{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build": "rimraf dist && nest build",
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.25.0",
    "aws-sdk": "^2.920.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.23",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.33"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^7.6.17",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.2",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "dtslint": "^4.1.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-sonarjs": "^0.7.0",
    "jest": "^27.0.3",
    "prettier": "^2.3.0",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "typeorm-model-generator": "^0.4.5-no-engines",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/node-nestjs-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}